{
    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET MAUI Android Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net9.0-android/SMSForwarder.dll",
            "cwd": "${workspaceFolder}",
            "presentation": {
                "hidden": false,
                "group": "MAUI",
                "order": 1
            },
            "internalConsoleOptions": "openOnSessionStart"
        }
    ]
}